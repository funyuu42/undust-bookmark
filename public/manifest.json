
{
  "manifest_version": 3,
  "name": "Undust bookmarks",
  "version": "1.0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "author": "funyuu42@gmail.com",  
  "description": "Allows you to randomly check ignored bookmarks during your leisure time",
  "action": {
    "default_popup": "popup.html",
    "default_icon":{
      "16":"favicon16.png",
      "32":"favicon32.png"
    }
  },
  "icons": {},
  "background.service_worker": {
    "scripts": ["background/background.js"]
  },
  "permissions": [
    "favicon",
    "history",
    "bookmarks",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ]
}
